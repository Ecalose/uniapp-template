{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^tm-(.*)": "@/tmui/components/tm-$1/tm-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/count/count",
      "type": "page",
      "style": {
        "navigationBarTitleText": "Count"
      }
    },
    {
      "path": "pages/not-found/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "NotFound"
      },
      "name": "notfound"
    },
    {
      "path": "pages/test/test",
      "type": "page",
      "style": {}
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarBackgroundColor": "#777",
    "navigationBarTitleText": "uniapp-vue3"
  },
  "tabBar": {
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "Home"
      },
      {
        "pagePath": "pages/count/count",
        "text": "Count"
      }
    ],
    "custom": true
  }
}