{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "~/*": [
        "./*"
      ]
    },
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "@dcloudio/types",
      "vite/client",
      "node",
      "@uni-helper/uni-app-types",
      "pinia-plugin-persist-uni",
      "miniprogram-api-typings"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
